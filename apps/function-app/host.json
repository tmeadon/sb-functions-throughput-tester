{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "Host.Concurrency": "Trace"
        }
    },
    "concurrency": { 
        "dynamicConcurrencyEnabled": true, 
        "snapshotPersistenceEnabled": true 
    }
}